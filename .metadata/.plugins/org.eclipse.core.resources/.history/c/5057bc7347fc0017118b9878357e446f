package com.name.test;

public class ReverseString {

	public static void main(String[] args) {

		String s1 = "Sonu is Talented";

		for (String w : s1.split("\\s", 0)) {
			for (int i = s1.length() - 1; i >= 0; i--) {
				char c = s1.charAt(i);
				System.out.print(c);
			}
			System.out.println(" ");
		}
	}
}